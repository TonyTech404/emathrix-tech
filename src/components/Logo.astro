---
import { SITE } from 'astrowind:config';
import { Image } from 'astro:assets';
import logoImage from '~/assets/images/eMathrix_logo.png';
---

<div class="flex items-center">
  <Image 
    src={logoImage} 
    alt={SITE?.name || 'eMathrix TECHNOLOGIES'} 
    class="w-auto h-10 sm:h-12" 
    width={200} 
    height={50} 
    loading="eager"
  />
</div>
